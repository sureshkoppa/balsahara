<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@taglib  uri="/struts-tags" prefix="s"%>
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <!--
         private String homeId;
    private String homeAddress;
    private String homeType;
    private String homeDistrict;
    private String homeManagedBy;
    private String contactPerson;
    private String contactNo;
        -->
    </head>
    <body>
        <p>
            <s:if test="homeDetailsDTO != null">
            <s:push value="homeDetailsDTO">
            <table>
                <tr>
                    <td align="left">Home Id:</td>
                    <th align="left" bgcolor="Silver"><b><s:property value="homeId"/><br></b></th>
                </tr>
                 <tr>
                    <td align="left">Home Address:</td>
                    <th align="left" bgcolor="Silver"><b><s:property value="homeAddress"/><br></b></th>
                </tr>
                 <tr>
                    <td align="left">Managed By:</td>
                    <th align="left" bgcolor="Silver"><b><s:property value="homeManagedBy"/><br></b></th>
                </tr>
                 <tr>
                    <td align="left">Contact Person:</td>
                    <th align="left" bgcolor="Silver"><b><s:property value="contactPerson"/><br></b></th>
                </tr>
                 <tr>
                    <td align="left">Contact No:</td>
                    <th align="left" bgcolor="Silver"><b><s:property value="contactNo"/><br></b></th>
                </tr>
            </table>
                 
                 
                 
                 
                
             
                               
            </s:push>
            </s:if>
        </p>
    </body>
</html>
